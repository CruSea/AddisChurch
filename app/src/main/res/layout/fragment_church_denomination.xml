<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.gcme.addischurch.addischurch.Fragments.ChurchDenomination">

    <RelativeLayout
        android:id="@+id/appbar3"
        android:layout_width="wrap_content"
        android:layout_height="50dp">

        <com.arlib.floatingsearchview.FloatingSearchView
            android:id="@+id/floating_search_view3"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:floatingSearch_searchHint="Search..."
            app:floatingSearch_suggestionsListAnimDuration="250"
            app:floatingSearch_showSearchKey="false"
            app:floatingSearch_leftActionMode="showHamburger"

            app:floatingSearch_menu="@menu/menu_main"
            app:floatingSearch_close_search_on_keyboard_dismiss="true"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"/>

        <!--<android.support.v4.widget.NestedScrollView-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:clipToPadding="false"-->
        <!--app:layout_behavior="@string/appbar_scrolling_view_behavior">-->

        <!--<TextView-->
        <!--android:clipToPadding="false"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="randomtext"-->
        <!--android:textSize="18sp"/>-->
        <!--</android.support.v4.widget.NestedScrollView>-->

    </RelativeLayout>

    <ListView

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:divider="#00000000"
        android:dividerHeight="3dp"
        android:id="@+id/search_suggestion_list"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        android:layout_below="@+id/appbar3"
        app:layout_constraintHorizontal_bias="0.33" />

    <ListView

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#00000000"
        android:dividerHeight="3dp"
        android:id="@+id/activelist"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        app:layout_constraintHorizontal_bias="0.33"
        android:layout_below="@+id/appbar3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
